{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run PyQuit Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": ["--extensionDevelopmentPath=${workspaceFolder}/apps/extension"],
      "outFiles": ["${workspaceFolder}/apps/extension/out/**/*.js"]
    }
  ]
}

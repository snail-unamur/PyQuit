{
  "name": "pyquit",
  "private": true,
  "packageManager": "pnpm@8.8.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "dev:docs": "pnpm --filter @pyquit/docs dev",
    "build:rules": "pnpm --filter @pyquit/rules build",
    "build:extension": "pnpm --filter pyquit build"
  },
  "devDependencies": {
    "turbo": "^2.0.0",
    "typescript": "^5.4.0"
  }
}
